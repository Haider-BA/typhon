!------------------------------------------------------------------------------!
! MENU TYPHON 
!------------------------------------------------------------------------------!

BLOCK:PROJECT           ! DEFINITION DU PROJET
  NZONE     = 1       
  COORD     = 2D      
  TIME      = STEADY 
  RESIDUALS = 1.e-6
ENDBLOCK               

BLOCK:OUTPUT            ! SORTIE FICHIERS DES RESULTATS
  FORMAT = VTK         
  FILE   = "typhon.vtk" 
ENDBLOCK

BLOCK:ZONE              ! DEFINITION DES ZONES ( * nb zones )
  NAME   = "bicyl" 
  SOLVER = EULER  
ENDBLOCK

!------------------------------------------------------------------------------!
! DEFINITION DE LA ZONE (si NZONE = 1, et FILE inexistant dans BLOCK:ZONE)
!------------------------------------------------------------------------------!

BLOCK:MESH
  FORMAT = CGNS
  FILE   = "bicyl.cgns"
ENDBLOCK

BLOCK:MODEL
  DYNAMICS = EULER
  GAS      = AIR
ENDBLOCK

BLOCK:TIME_PARAM 
  METHOD    = IMPLICIT
  INVERSION = BICGSTAB
  INV_RES   = 1.e-1
  MAX_IT    = 100
  CFL       = 4.
  CFL_MAX   = 100.
ENDBLOCK

BLOCK:SPAT_PARAM  
  SCHEME   = HLLC
  JACOBIAN = HLL-diag
  !HIGHRES  = MUSCL
  !LIMITER  = MINMOD
ENDBLOCK

BLOCK:INIT
  PI      = 1.E5
  TI      = 300.
  MACH    = 10.
  DIRECTION = (1., 0., 0.)
ENDBLOCK

BLOCK:BOCO
  FAMILY    = UPSTREAM
  TYPE      = SUPERSONIC_INLET
  PI        = 1.E5
  TI        = 300.
  MACH      = 10.
  DIRECTION = (1., 0., 0.)
ENDBLOCK

BLOCK:BOCO
  FAMILY = CYL1
  TYPE   = SYMMETRY
ENDBLOCK

BLOCK:BOCO
  FAMILY = CYL2
  TYPE   = SYMMETRY
ENDBLOCK

BLOCK:BOCO
  FAMILY = DOWNSTREAM
  TYPE   = SUPERSONIC_OUTLET 
ENDBLOCK



