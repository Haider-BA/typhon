!------------------------------------------------------------------------------!
! MENU TYPHON V 0.0.1b
!------------------------------------------------------------------------------!

BLOCK:PROJECT           ! DEFINITION DU PROJET
 !MEMMODE = NORMAL      ! optimisation mémoire : NORMAL / SAVECPU / SAVEMEMORY !
                        !   (faculatif, defaut = NORMAL)
  NZONE   = 1           ! nombre de zones (facultatif, défaut = 1) 
  NCOUPLING = 0         ! nombre de raccords
  COORD   = 3D          ! type de repère  : 2D=2DPLAN / 2DAXI / 3D
  TIME    = UNSTEADY    ! type d'intégration temporelle : STEADY / UNSTEADY / PERIODIC
  DURATION  = 1000        ! UNSTEADY : durée totale du calcul (temps physique)
  BASETIME  = 0.1       ! UNSTEADY : temps de base pour le couplage ou temps de cycle
ENDBLOCK               

BLOCK:OUTPUT
  FORMAT = TECPLOT
  FILE = "typhon.dat"
  TYPE = CENTER
ENDBLOCK

BLOCK:ZONE              ! DEFINITION DES ZONES ( * nb zones )
  NAME   = "zone1"       ! nom de la zone (facultatif)
  SOLVER = HEAT         ! type de solveur : HEAT / EULER / NS
  FILE   = "zone1.rpm"         ! nom du fichier RPM pour la définition de la zone (facultatif)
ENDBLOCK

