!------------------------------------------------------------------------------!
! MENU TYPHON 
!------------------------------------------------------------------------------!

BLOCK:PROJECT           ! DEFINITION DU PROJET
  COORD     = 2D        ! type de repère  : 2D=2DPLAN / 2DAXI / 3D
  TIME      = STEADY    ! type d'intégration temporelle : STEADY / UNSTEADY / PERIODIC
  RESIDUALS = 1.E-5     ! STEADY : résidu maximal dans le champ
ENDBLOCK               

BLOCK:OUTPUT            ! SORTIE FICHIERS DES RESULTATS
  FORMAT = TECPLOT      ! format du fichier de sortie : RAW / TECPLOT / VIGIE
  !TYPE   = CENTER
  FILE   = "typhon.dat" ! nom du fichier de sortie
ENDBLOCK

BLOCK:ZONE              ! DEFINITION DES ZONES ( * nb zones )
  NAME   = "CYL"        ! nom de la zone (facultatif)
  SOLVER = HEAT         ! type de solveur : HEAT / EULER / NS
ENDBLOCK

!------------------------------------------------------------------------------!
! DEFINITION DE LA ZONE (si NZONE = 1, et FILE inexistant dans BLOCK:ZONE)
!------------------------------------------------------------------------------!

BLOCK:MESH
  FORMAT = CGNS
  FILE   = "cyl-qtri20.cgns"
ENDBLOCK

BLOCK:MODEL
  MATERIAL = DEFINITION
  CONDUCT  = .01
  HEATCAPA = 10.
ENDBLOCK

BLOCK:TIME_PARAM
  FOURIER = 0.48
ENDBLOCK

BLOCK:SPAT_PARAM
  DISSIPATIVE_FLUX = AVERAGE
ENDBLOCK

BLOCK:INIT
  TEMP = 300.
ENDBLOCK

BLOCK:BOCO
  FAMILY = SYM
  TYPE   = EXTRAPOLATE
  ORDER  = QUANTITY         ! QUANTITY (ordre 1) ou GRADIENT (ordre 2)
ENDBLOCK

BLOCK:BOCO
  FAMILY      = EXT
  TYPE        = ISOTHERMAL_WALL
  WALL_TEMP   = 500.
ENDBLOCK

BLOCK:BOCO
  FAMILY      = INT
  TYPE        = ISOTHERMAL_WALL
  WALL_TEMP   = 100.
ENDBLOCK

