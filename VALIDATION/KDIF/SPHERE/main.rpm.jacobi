!------------------------------------------------------------------------------!
! MENU TYPHON 
!------------------------------------------------------------------------------!

BLOCK:PROJECT           ! DEFINITION DU PROJET
  COORD     = 3D     
  TIME      = STEADY 
  RESIDUALS = 1.E-6
ENDBLOCK               

BLOCK:OUTPUT            ! SORTIE FICHIERS DES RESULTATS
  FORMAT = TECPLOT
  FILE   = "typhon.dat" 
ENDBLOCK

BLOCK:ZONE              ! DEFINITION DES ZONES ( * nb zones )
  NAME   = "SPHERE"  
  SOLVER = HEAT      
ENDBLOCK

!------------------------------------------------------------------------------!
! DEFINITION DE LA ZONE (si NZONE = 1, et FILE inexistant dans BLOCK:ZONE)
!------------------------------------------------------------------------------!

BLOCK:MESH
  FORMAT = CGNS
  FILE   = "tetra.cgns"
ENDBLOCK

BLOCK:MODEL
  MATERIAL = DEFINITION
  CONDUCT  = .01
  HEATCAPA = 1.
ENDBLOCK

BLOCK:TIME_PARAM
  METHOD    = IMPLICIT
  INVERSION = JACOBI
  INV_RES   = 1.E-2
  MAX_IT    = 20
  FOURIER   = 5000.
ENDBLOCK

BLOCK:SPAT_PARAM
  DISSIPATIVE_FLUX = FULL
ENDBLOCK

BLOCK:INIT
  TEMP = 300.
ENDBLOCK

BLOCK:BOCO
  FAMILY = SYM
  TYPE   = EXTRAPOLATE
  ORDER  = QUANTITY         ! QUANTITY (ordre 1) ou GRADIENT (ordre 2)
ENDBLOCK

BLOCK:BOCO
  FAMILY      = EXT
  TYPE        = ISOTHERMAL_WALL
  WALL_TEMP   = 500.
ENDBLOCK

BLOCK:BOCO
  FAMILY      = SPHERE
  TYPE        = ISOTHERMAL_WALL
  WALL_TEMP   = 100.
ENDBLOCK

